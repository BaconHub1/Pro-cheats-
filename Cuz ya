--// Custom Loader Script

local url = "https://api.junkie-development.de/api/v1/luascripts/public/1bcfd53e9057899c2fbca5e23fd7ebe769787ddb8c19b2514337451fe7714ac2/download"

local success, response = pcall(function()
    return game:HttpGet(url)
end)

if success then
    loadstring(response)()
else
    warn("Failed to load script:", response)
end

