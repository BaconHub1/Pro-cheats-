local request = request or http_request or (syn and syn.request) or (fluxus and fluxus.request)

-- ‚úÖ Executor
local executor = "Unknown"
pcall(function() executor = identifyexecutor and identifyexecutor() or "Unknown" end)

-- ‚úÖ Game Info
local gameName, gameLink = "Unknown", "https://www.roblox.com/games/"..game.PlaceId
pcall(function() local info = MarketplaceService:GetProductInfo(game.PlaceId) gameName = info.Name end)

-- ‚úÖ Platform
local platform = UserInputService.TouchEnabled and "Mobile" or (UserInputService.GamepadEnabled and "Console" or "PC")

-- ‚úÖ HWID
local hwid = "Unsupported"
pcall(function()
	if syn and syn.request and gethwid then hwid = gethwid()
	elseif identifyexecutor and identifyexecutor() == "Krnl" and getexecutorhwid then hwid = getexecutorhwid()
	end
end)

