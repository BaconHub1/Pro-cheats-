--// Custom Loader Script

local url = "loadstring(game:HttpGet("https://api.junkie-development.de/api/v1/luascripts/public/28d5d771739de2545924df85303ea3350b682438833c611108f4b08bd0455d05/download"))()"

local success, response = pcall(function()
    return game:HttpGet(url)
end)

if success then
    loadstring(response)()
else
    warn("Failed to load script:", response)
end
